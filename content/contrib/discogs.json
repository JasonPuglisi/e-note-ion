{
  "templates": {
    "morning_spin": {
      "schedule": {
        "cron": "0 8 * * *",
        "hold": 600,
        "timeout": 3600
      },
      "priority": 5,
      "truncation": "ellipsis",
      "integration": "discogs",
      "templates": [
        {
          "format": [
            "[Y] MORNING SPIN",
            "{album}",
            "{artist}"
          ]
        }
      ]
    }
  }
}
